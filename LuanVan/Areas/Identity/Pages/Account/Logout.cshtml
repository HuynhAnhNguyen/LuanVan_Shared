@page
@model LogoutModel
@{
    ViewData["Title"] = @_localization.Getkey("Logout");
}


<div class="breadcrumbs">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-6 col-12">
                <div class="breadcrumbs-content">
                    <h1 class="page-title">@_localization.Getkey("Logout")</h1>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-12">
                <ul class="breadcrumb-nav">
                    <li>
                        <a href="/Store/Home/Index"><i class="lni lni-home"></i> @_localization.Getkey("Home")</a>
                    </li>
                    <li>@_localization.Getkey("Logout")</li>
                </ul>
            </div>
        </div>
    </div>
</div>

<div class="account-login section">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 offset-lg-3 col-md-10 offset-md-1 col-12">
                <div class="form-control">
                    <div class="card-body">
                        <div class="title">
                            <h3>@_localization.Getkey("Logout")</h3>
                            <p>
                                @_localization.Getkey("LogoutTitle")
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




